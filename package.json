    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.14",
    "lodash": "^4.17.20",
    "pg": "^7.14.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-sorted": "^0.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2"
  }
